<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/23" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ma.ensa.lis.controllers.AjouterMembreController">
    <children>
        <Text layoutX="170.0" layoutY="30.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Ajouter un nouveau membre">
            <font>
                <Font name="Arial" size="24.0" />
            </font>
        </Text>
        <Label layoutX="50.0" layoutY="90.0" text="Nom" />
        <TextField layoutX="150.0" fx:id="nom" layoutY="85.0" prefHeight="26.0" prefWidth="380.0" />

        <Label layoutX="50.0" layoutY="140.0" text="Prénom" />
        <TextField layoutX="150.0" fx:id="prenom" layoutY="135.0" prefHeight="26.0" prefWidth="380.0" />

        <Label layoutX="50.0" layoutY="190.0" text="Email" />
        <TextField layoutX="150.0" fx:id="email" layoutY="185.0" prefHeight="26.0" prefWidth="380.0" />

        <Label layoutX="50.0" layoutY="240.0" text="Téléphone" />
        <TextField layoutX="150.0" fx:id="phone" layoutY="235.0" prefHeight="26.0" prefWidth="380.0" />

        <Button layoutX="150.0" layoutY="300.0" onAction="#AjouterMembre" mnemonicParsing="false" prefHeight="30.0" prefWidth="130.0" text="Insérer" />
        <Button layoutX="400.0" layoutY="300.0" mnemonicParsing="false" prefHeight="30.0" prefWidth="130.0" text="Retour" />
    </children>
</AnchorPane>
